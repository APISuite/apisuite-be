{
  "name": "apisuite-be",
  "version": "5.4.1",
  "description": "APISuite backend core API",
  "main": "app.js",
  "scripts": {
    "lint": "npx eslint \"**/*.js\"",
    "local": "node app.js | pino-pretty",
    "migrate": "npx sequelize-cli db:migrate --url \"postgres://$POSTGRES_USERNAME:$POSTGRES_PASSWORD@$POSTGRES_ADDRESS:$POSTGRES_PORT_INTERNAL/$POSTGRES_DB\"",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --url \"postgres://$POSTGRES_USERNAME:$POSTGRES_PASSWORD@$POSTGRES_ADDRESS:$POSTGRES_PORT_INTERNAL/$POSTGRES_DB\"",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --url \"postgres://$POSTGRES_USERNAME:$POSTGRES_PASSWORD@$POSTGRES_ADDRESS:$POSTGRES_PORT_INTERNAL/$POSTGRES_DB\"",
    "start": "node app.js",
    "test": "NODE_ENV=test mocha {routes,controllers,services}/**/*.spec.js --inspect=7000 --exit"
  },
  "pre-commit": [
    "audit",
    "lint"
  ],
  "author": "APISuite",
  "license": "MPL-2",
  "dependencies": {
    "@apidevtools/swagger-parser": "10.0.3",
    "@awaitjs/express": "0.9.0",
    "@aws-sdk/client-s3": "3.45.0",
    "accesscontrol": "2.2.1",
    "amqp-connection-manager": "3.8.1",
    "amqplib": "0.8.0",
    "bcrypt": "5.0.1",
    "convict": "6.2.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "deepmerge": "4.2.2",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "express-basic-auth": "^1.2.1",
    "express-prom-bundle": "6.4.1",
    "file-type": "16.5.3",
    "formidable": "2.0.1",
    "handlebars": "4.7.7",
    "http-status-codes": "2.2.0",
    "iso-lang-codes": "2.1.1",
    "joi": "17.5.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.5",
    "moment": "2.29.1",
    "morgan": "1.10.0",
    "node-fetch": "2.6.1",
    "nodemailer": "6.7.2",
    "pg": "8.7.1",
    "pino": "7.6.2",
    "prom-client": "14.0.1",
    "sequelize": "6.6.5",
    "swagger-jsdoc": "6.1.0",
    "swagger2openapi": "7.0.8",
    "url": "0.11.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chance": "^1.1.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.1.3",
    "mock-req-res": "^1.2.0",
    "pino-pretty": "^7.3.0",
    "semantic-release": "^18.0.1",
    "sequelize-cli": "6.3.0",
    "sinon": "^12.0.1"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [ci skip]"
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failComment": false
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
